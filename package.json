{
  "name": "github-pr-dashboard",
  "version": "0.0.0",
  "description": "Overview of pull requests for GitHub repository",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "build": "npm run clean && webpack --progress --profile --colors",
    "dev": "webpack-dev-server --progress --profile --colors --hot --inline --history-api-fallback",
    "lint": "eslint --ext js --ext jsx src",
    "prepush": "npm run lint",
    "precommit": "npm run lint"
  },
  "keywords": [
    "github",
    "pull",
    "request",
    "dashboard"
  ],
  "author": "Visya",
  "license": "MIT",
  "dependencies": {
    "material-ui": "^0.16.6",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-tap-event-plugin": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.25.0",
    "husky": "^0.12.0",
    "react-hot-loader": "^1.3.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
