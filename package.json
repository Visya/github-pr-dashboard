{
  "name": "dashboard",
  "version": "0.0.0",
  "description": "Admin dashboard for Flow Networks service",
  "browsersList": [
    "> 1%",
    "last 2 versions"
  ],
  "engineStrict": true,
  "engines": {
    "npm": ">= 3",
    "node": ">= 6"
  },
  "scripts": {
    "build": "npm run clean && webpack --config webpack.prod.config.babel.js --progress --profile --colors",
    "clean": "rm -rf dist",
    "dev": "webpack-dev-server --progress --profile --colors --hot --inline --history-api-fallback",
    "lint:css": "stylelint \"app/**/*.scss\"",
    "lint:js": "eslint --ext js --ext jsx app",
    "lint": "npm run lint:js",
    "prepush": "npm run lint",
    "precommit": "npm run lint",
    "test": "echo 'No tests are specified'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-toolbox": "^2.0.0-beta.6",
    "recompose": "^0.22.0",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-config-tough": "^0.4.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.1",
    "istanbul-instrumenter-loader": "^2.0.0",
    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "postcss": "^5.2.13",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.0",
    "postcss-reporter": "^3.0.0",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-webpack-plugin": "^0.6.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
