    "lint": "eslint --ext js --ext jsx src",
    "prepush": "npm run lint",
    "precommit": "npm run lint"
  },
  "keywords": [
    "github",
    "pull",
    "request",
    "dashboard"
  ],
  "author": "Visya",
  "license": "MIT",
